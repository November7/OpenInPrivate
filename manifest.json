{  
	"author" : "Marcin Kowalski",
	"name": "OpenInPrivate",
	"description": "Open link in private window",
	"version": "1.0.0",
	"permissions": ["contextMenus","storage"],
	"background": 
	{
		"persistent": true,
		"scripts": ["bkg.js"]
	},	
	"icons" : {      
        "48" : "ico/icon_48.png",
		"16" : "ico/icon_16.png"
    },
	"browser_action": 
	{
		"default_icon": 
		{
			"20": "ico/icon_20.png",        	
			"38": "ico/icon_38.png",
			"40": "ico/icon_40.png"
		},
		"default_title": "__MSG_optionPage__",
		"default_popup": "popup.html"
	},
    "default_locale" : "en"
}
